{
  "$schema": "../../app.schema.json",
  "pages": ["pages/home/index"],
  "window": {},
  "lazyCodeLoading": "requiredComponents",
  "requiredBackgroundModes": ["audio", "location"],
  "usingComponents": {
    "hello_world": "w1-xx/hello/world"
  },
  "supportedMaterials": [
    {
      "materialType": "audio/*",
      "desc": "1234567890123456789012",
      "name": "foo${nickname}bar",
      "path": "pages/home/index"
    }
  ],
  "preloadRule": {
    "hello": {
      "packages": ["hello"]
    }
  },
  "plugins": {
    "hello": {
      "version": "1.2.3",
      "provider": "ssdsadasdasd"
    }
  },
  "permission": {
    "scope.address": {
      "desc": "Required"
    }
  },
  "chatTools": [
    {
      "root": "foobar",
      "entryPagePath": "pages/home/index",
      "desc": "foobar",
      "scopes": ["scope.addPhoneCalendar", "scope.addPhoneContact"]
    }
  ],
  "resolveAlias": {},
  "resizable": false,
  "darkmode": true,
  "themeLocation": "./theme.json"
}
