    "ajv": "^8.17.1",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,json,yml,yaml,md}": "eslint --fix"
